<div class="py-5 ml-3 mx-auto">
    <% if user_signed_in? %>
        <%= link_to "log out", destroy_user_session_path, data: {turbo_method: :delete,
        turb_frame: "_top"} %>
        <%= link_to 'Edit profile', edit_user_registration_path, data_turbo_frame: "_top" %>
        <%= current_user.username%>
    <% else %>
        <%= link_to "log in", new_user_session_path, data_turbo_frame: "_top"  %>
        <%= link_to "Sign up", new_user_registration_path, data_turbo_frame: "_top"  %>
    <% end %>
</div>